/*default link colors*/
a:link,a:visited{text-decoration:none;color:#336699;}

/**vertical tabs  */
.ui-tabs-vertical {}
.ui-tabs-vertical .ui-tabs-nav { padding: 10px; float: left; }
.ui-tabs-vertical .ui-widget-header{border:none;}
.ui-tabs-vertical .ui-tabs-nav li { clear: left; width: 100%; border-bottom:1px dotted green !important ;border-right-width: 0 !important; margin: 0 -1px .2em 0; }
.ui-tabs-vertical .ui-tabs-nav li a { display:block; }
.ui-tabs-vertical .ui-tabs-nav li.ui-tabs-selected { padding-bottom: 0; padding-right: .1em; border-right-width: 1px; border-right-width: 1px; }
.ui-tabs-vertical .ui-tabs-panel { padding: 1em; float: right; }
.ui-tabs-vertical .ui-state-default{background:transparent !important;}
.ui-tabs-vertical .ui-state-active{background:transparent !important;}
.ui-tabs-vertical .ui-state-active a{color:#000 !important;font-weight:bold;}
.verticaltab { padding: 10px 6px !important;}
.verticaltab a {font-size:13px; }

